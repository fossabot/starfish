{
  "name": "starfish",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": ">=16.0.1"
  },
  "scripts": {
    "v": "node -v",
    "dev": "npm run dcu",
    "up": "npm run dcu",
    "dcu": "docker-compose up",
    "down": "docker-compose down",
    "produp": "docker-compose -f docker-compose-prod.yml up",
    "hard-restart": "docker-compose down --remove-orphans --volumes && docker-compose up -d",
    "restart": "docker-compose down --remove-orphans && docker-compose up -d",
    "mongo": "mongod --dbpath ./db/data",
    "circularcheck": "npx madge --circular ./game",
    "cypress:run": "cypress run --headed --browser chrome",
    "cypress:runheadless": "cypress run --record --key 99ff6c7c-528e-4b9f-8c3f-0fdb6bd179ec"
  },
  "devDependencies": {
    "@types/mongoose": "5.11.97",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "cypress": "^8.6.0",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nuxt-font-loader": "^1.1.3",
    "ts-node": "^10.2.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "browserslist": "^4.17.5",
    "nodemon": "^2.0.7",
    "stdio": "^2.1.1"
  }
}
